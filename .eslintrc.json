{
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "max-len": ["warn", { "code": 160 }],
    "no-await-in-loop": "warn",
    "no-console":"off",
    "no-param-reassign":"off",
    "no-restricted-syntax": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": ["error", {
        "args": "none" 
      }],
    "import/prefer-default-export": "off"
  }
}